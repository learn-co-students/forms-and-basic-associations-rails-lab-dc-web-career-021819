<h1>Create a Song</h1>
<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br>
  <select name="song_genre_id">
    <% Genre.all.each do |g| %>
      <option value="<%=g.id%>"><%= g.name %></option>
    <% end %>
  </select>
  <br>
  <%= label_tag :song_notes_1 %>
  <%= text_field_tag :song_notes_1 %>
  <br>
  <%= label_tag :song_notes_2 %>
  <%= text_field_tag :song_notes_2 %>
  <%= f.submit %>
<% end %>
