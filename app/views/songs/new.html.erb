<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  </br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre_id %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>
  <%= f.label :notes %>
  <input id="song_notes_1" type="text" name="song[note_contents][]" value="">
  <input id="song_notes_2" type="text" name="song[note_contents][]" value="">
  <%= f.submit %>
<% end %>
